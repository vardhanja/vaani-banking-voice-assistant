# Backend production environment example
# Copy to .env.production (not committed) and adjust values before deployment.

# Database
DB_BACKEND=postgresql
DATABASE_URL=postgresql://<user>:<password>@<host>/<database>?sslmode=require

# Application secrets
JWT_SECRET_KEY=change-me
ACCESS_TOKEN_EXPIRE_MINUTES=30

# External services
AI_BASE_URL=https://ai.sunnationalbank.online
OPENAI_API_KEY=<openai-key-if-backend-needs-direct-access>

# CORS overrides (comma-separated origins added to defaults)
CORS_ALLOWED_ORIGINS=https://sunnationalbank.online,https://www.sunnationalbank.online

# Feature flags
VOICE_VERIFICATION_ENABLED=false

# Optional logging overrides
LOG_LEVEL=INFO
